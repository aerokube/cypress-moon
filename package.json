{
  "name": "@aerokube/cypress-moon",
  "version": "1.0.7",
  "moonVersion": "2.0.0",
  "description": "Binary installer to use Cypress with Moon",
  "repository": "aerokube/cypress-moon",
  "author": "Ivan Krutov <vania-pooh@aerokube.com>",
  "license": "Apache-2.0",
  "main": "index.js",
  "bin": {
    "cypress-moon": "cli.js"
  },
  "dependencies": {
    "follow-redirects": "^1.14.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "bin-check": "^4.1.0",
    "eslint": "^7.27.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0"
  },
  "scripts": {
    "eslint": "eslint .",
    "mocha": "mocha",
    "test": "npm run eslint && npm run mocha",
    "postinstall": "rimraf vendor && node lib/install"
  },
  "files": [
    "lib/*.js",
    "cli.js",
    "index.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2017": true,
      "node": true
    },
    "rules": {
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ],
      "strict": "error"
    }
  }
}
